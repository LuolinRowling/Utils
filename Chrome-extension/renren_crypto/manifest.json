{
    "name": "人人作死精灵",
    "version": "0.1",
    "description": "水表在门外，没叫外卖，没有快递！不作死就不会死！",
    "permissions": [
        "tabs", 
        "http://*/*"
    ],
    "content_scripts": [{
        "matches": ["http://*.renren.com/*"],
        "js": ["jquery.js","crypto.js"],
        "run_at": "document_end"
    }],
    "web_accessible_resources": [
    ],
    "browser_action":{
        "default_title": "人人作死精灵",
        "default_icon": "icon.png"
    },
    "icons":{
        "16": "icon16.png",
        "48": "icon48.png",
        "128":"icon128.png" 
    },
    "web_accessible_resources": ["script.js"],
    "manifest_version": 2
}
